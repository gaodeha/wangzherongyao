<style lang="scss">
  .container {
    color: white;
  }

  .audio-switch {
    position: fixed;
    top: 0;
    right: 20rpx;
    width: 80rpx;
    height: 80rpx;
    image {
      width: 60rpx;
      height: 60rpx;
    }
  }

  .top-tips {
    height: 80rpx;
    width: 100%;
    line-height: 80rpx;
    text-align: center;
    background-color: #1c2b42;
    color: #888888;
    text {
      margin-left: 20rpx;
    }
  }

  .q-index {
    font-size: 36rpx;
    margin: 0 3px;
    color: #dd9e1b;
  }

  .count-down {
    width: 80px;
    height: 80px;
    .canvas {
      width: 80px;
      height: 80px;
    }
  }

  .question {
    min-height: 240rpx;
  }

  .answer-item {
    height: 76rpx;
    border-radius: 40rpx;
    margin: 10rpx 0 20rpx 0;
    font-size: 14px;
    width: 90%;
    background: #4a596f;
  }

  .answer-choose {
    transition: all 0.3s ease-out;
    transform: scale(1.04);
    background: linear-gradient(to right, #4a596f, #4f5f6f);
  }


  .answer-correct {
    background: #65c038;
    position: relative;

    .answer-status {
      background-image: url(data:image/png;base64,\x3d\x3d);
    }
  }

  .answer-error {
    background: #ff7d52;
    position: relative;
    .answer-status {
      background-image: url(data:image/png;base64,);
    }
  }

  .answer-status {
    position: absolute;
    top: 20rpx;
    right: 20rpx;
    width: 36rpx;
    height: 36rpx;
    background-size: cover;
  }

  .modal {
    position: absolute;
    top: 200rpx;
    left: 50rpx;
    right: 50rpx;
    z-index: 100;
    border-radius: 6rpx;
  }

  .result-error {
    background-color: #FFFFFF;
    .close {
      position: absolute;
      right: 0rpx;
      top: 0rpx;
      width: 80rpx;
      height: 80rpx;

      image {
        width: 40rpx;
        height: 40rpx;
      }
    }

    .reason {
      margin-top: 60rpx;
      color: red;
      font-size: 32rpx;
      image {
        width: 120rpx;
        height: 120rpx;
      }
    }

    .reason-tips {
      margin-top: 60rpx;
      color: #888888;
      font-size: 24rpx;
    }
  }

  .result-success {
    top: 100rpx;
    height: 844rpx;
    background-image: url(data:image/png;base64,\x3d); background-repeat: no-repeat; background-size: 100% 100%;

    image {
      width: 160rpx;
      height: 160rpx;
      border-radius: 50%;
    }

    .success-tips {
      color: #E79676;
      margin-top: 40rpx;
    }

    .draw-red {
      width: 120rpx;
      height: 120rpx;
      position: absolute;
      bottom: 100rpx;
      background: #F2C949;
      border-radius: 50%;
    }
  }

  .btn-group {
    width: 80%;
    margin-top: 30rpx;
    margin-bottom: 40rpx;
    .btn {
      width: 100%;
      height: 88rpx;
      font-size: 32rpx;
      border-radius: 10rpx;
    }

    .share-btn {
      background-color: #FD5D5C;
      color: #FFFFFF;
    }

    .goon-btn {
      margin-top: 24rpx;
      background-color: #F0F1F3;
      color: #666666;
      border: 1px solid #cccccc;
    }

  }

  .text-reverse {
    color: #ffd700
  }


</style>
<template>
  <view class="container flex flex-column">

      <view class="audio-switch flex justify-center align-center">
        <image wx:if="{{audioOn}}" src="/images/audio_on.png" @tap="switchAudio"></image>
        <image wx:else src="/images/audio_off.png" @tap="switchAudio"></image>
      </view>

      <view class="top-tips flex align-center">
        <text>答对{{qCount}}题得红包</text>
      </view>

      <view class="margin-top flex flex-column justify-between align-center">
        <view>
          <view class="flex align-center justify-center">第<text class="q-index">{{qIndex}}</text>题</view>
        </view>
        <view class="count-down" wx:if="{{showCountDown}}">
          <canvas canvas-id='countDown' class="canvas"></canvas>
        </view>
      </view>

      <view class="flex flex-grow flex-column align-center width-full justify-center">
            <view class="question flex flex-column flex-grow align-center justify-center">
                <view class="margin-left margin-right question-text text-align-center">
                    <reversetitle></reversetitle>
                </view>
                <view class="question-image" wx:if="{{question.imageUrl}}">
                  <image style="max-width: 640rpx; max-height: 300rpx;" mode="aspectFit" src="{{question.imageUrl}}"></image>
                </view>
            </view>
            <view class="answers flex-noshrink flex flex-column align-center justify-center width-full">
              <block wx:for="{{answers}}" wx:key="item.text" wx:if="{{!question.hasAnswered}}">
                  <view class="answer-item flex align-center justify-center margin-left margin-right"  @tap="chooseAnswer({{item.text}})">
                    {{item.text}}
                  </view>
              </block>
              <block wx:for="{{answers}}" wx:key="item.text" wx:if="{{question.hasAnswered}}">
                <view class="answer-item flex align-center justify-center margin-left margin-right {{item.statusClass}}">
                  {{item.text}}
                  <view wx:if="{{item.correct || item.choose }}" class="answer-status">

                  </view>
                </view>
              </block>
            </view>
      </view>

      <view class="modal result-error flex flex-column justify-center align-center" wx:if="{{resultStatus === 'error' || resultStatus === 'timeout'}}">

          <view class="close flex align-center justify-center" @tap="closeModal">
              <image src="/images/close1.png"></image>
          </view>

          <view class="reason flex flex-column align-center justify-center">
              <image src="/images/wrong.png"></image>
              <text>挑战失败-{{resultStatus === 'error'? '答题错误':'答题超时'}}</text>
          </view>

          <view class="reason-tips">
            <text>再接再厉咯~</text>
            <text>邀请好友可获得更多挑战机会</text>
          </view>

          <view class="result-buttom btn-group">
            <button open-type="share" class="btn share-btn flex align-center justify-center">邀请好友</button>
            <view class="btn goon-btn flex align-center justify-center" @tap="switchToIndex">再来一次</view>
          </view>
      </view>


      <view class="modal result-success flex flex-column justify-center align-center" wx:if="{{resultStatus === 'success'}}">

        <image src="/images/logo.jpeg"></image>

        <text class="success-tips">恭喜发财, 大吉大利</text>

        <view class="draw-red flex align-center justify-center" @tap="drawRedPacket">
          <text>拆红包</text>
        </view>
      </view>

      <audio src="https://wwwwzry.oss-cn-shenzhen.aliyuncs.com/countdown.mp3" id="countdownAudio"></audio>
      <audio src="https://wwwwzry.oss-cn-shenzhen.aliyuncs.com/right.mp3" id="rightTipsAudio"></audio>
      <audio src="https://wwwwzry.oss-cn-shenzhen.aliyuncs.com/timeover.mp3" id="timeoverAudio"></audio>
      <audio src="https://wwwwzry.oss-cn-shenzhen.aliyuncs.com/success.mp3" id="successAudio"></audio>
      <audio src="https://wwwwzry.oss-cn-shenzhen.aliyuncs.com/faild.mp3" id="faildAudio"></audio>
  </view>
</template>

<script>
  import wepy from 'wepy';
  import api from '@/api/api';
  import { delay } from "../utils/utils";
  import Config from '@/utils/config';
  import constants from '@/utils/constants';
  import Title from '@/components/title';

  // 倒计时定时任务
  let answerCountDownTimer = null;
  // 倒计时context
  let countDownCanvasContext = null;
  // 当前倒计时
  let maxCountSeconds = 12;
  let countDownSeconds = maxCountSeconds;

  let countdownAudioContext = null;
  let rightTipsAudioContext = null;
  let timeoverAudioContext = null;
  let successAudioContext = null;
  let faildAudioContext = null;


  export default class Answer extends wepy.page {
    config = {
    }
    data = {
      examId: 0,
      audioOn: true,
      showCountDown: true,
      resultStatus: "",
      qIndex: 0,
      qCount: 12,
      question: {},
      answers: []
    }

    computed = {
    }

    components = {
      reversetitle: Title
    }

    methods = {
      switchAudio() {
        this.audioOn = !this.audioOn;
        this.$parent.globalData.audioOn = this.audioOn;
      },
      chooseAnswer(choose) {
          this.userAnswerQustion(choose);
      },
      switchToIndex() {
        wx.switchTab({
          url: '/pages/index'
        })
      },
      drawRedPacket() {
        wx.redirectTo({
          url: '/pages/gift'
        });
      },
      closeModal() {
        this.resultStatus = "";
      }
    }

    onShareAppMessage() {
      console.log("onShareAppMessage");
      return this.$parent.getShareInfo();
    }

    onShow() {
      if (this.question.hasAnswered) {
        return;
      }
      this.initDataAfterShow();
      this.refreshConfigs();
      this.initSoundTips();
    }

    onHide() {
      this.destorySound();
    }


    initDataAfterShow() {
        this.examId = this.$parent.globalData.examId;
        this.audioOn = this.$parent.globalData.audioOn;
        this.resultStatus = "";
        this.fetchNextQuestion();
    }

    refreshConfigs() {
      this.qCount = this.$parent.globalData.qCount;
      maxCountSeconds = parseInt(Config.getWithDefault(constants.CONF_MAX_COUNT_DOWN, constants.CONF_MAX_COUNT_DOWN_DEFAULT));
    }

    /**
     * 初始化提示音
     */
    initSoundTips() {
      countdownAudioContext = wx.createAudioContext("countdownAudio");
      rightTipsAudioContext = wx.createAudioContext("rightTipsAudio");
      timeoverAudioContext = wx.createAudioContext("timeoverAudio");
      successAudioContext = wx.createAudioContext("successAudio");
      faildAudioContext = wx.createAudioContext("faildAudio");
    }

    destorySound() {
      countdownAudioContext = null;
      rightTipsAudioContext = null;
      timeoverAudioContext = null;
      successAudioContext = null;
      faildAudioContext = null;
    }

    async fetchNextQuestion() {
        const examQuestion = await api.getQuestion(this.examId, this.qIndex);
        const answersFromJson = JSON.parse(examQuestion.answers);
        const answers = answersFromJson.map(item => ({
          text: item,
          choose: false,
          correct: false,
          statusClass: ''
        }));
        const question = {
          'title': examQuestion.title,
          'imageUrl': examQuestion.imageUrl,
          'hasAnswered': false
        };
        this.reloadQuestion(question, answers);
    }

    reloadQuestion(question, answers) {
      this.question = question;
      this.answers = answers;
      this.qIndex += 1;
      this.$apply();
      this.$invoke('reversetitle', 'update', this.question.title);
      this.initCountDown();
    }

    userAnswerQustion(choose) {
        if (this.question.hasAnswered){
            return;
        }
        this.question.hasAnswered = true;
        this.clearTimer();
        this.answers.forEach(item => {
          item.choose = item.text === choose;
          if (item.choose) {
              item.statusClass = "answer-choose";
          } else {
              item.statusClass = "";
          }
        });
        this.answerQuestion(choose);
    }

    async answerQuestion(choose) {
        const r = api.answerQuestion(this.examId, this.qIndex - 1, choose);
        const delayedRequest = delay(0.3).then(() => r);
        const answer = await delayedRequest;
        this.answers.forEach(item => {
          item.correct = item.text === answer.correct;
          if (item.correct) {
            item.statusClass = "answer-correct";
          } else if (item.choose) {
            item.statusClass = "answer-error";
          } else {
            item.statusClass = "";
          }
        });
        if(answer.isCorrect || answer.choose === answer.correct){
          this.audioOn && rightTipsAudioContext.play();
        }
        this.$apply();
        delay(0.3).then( () => {
          if(answer.passedFlag){
            this.showSuccessModal();
          }else if(answer.correctFlag) {
            this.fetchNextQuestion();
          } else {
            this.showErrorModal();
          }
        });
    }

    showSuccessModal() {
      this.showCountDown = false;
      this.resultStatus = "success";
      this.audioOn && successAudioContext.play();
      this.$apply();
    }

    showErrorModal() {
      this.showCountDown = false;
      this.resultStatus = "error";
      this.audioOn && faildAudioContext.play();
      this.$apply();
    }

    showTimeoutModal() {
      this.showCountDown = false;
      this.resultStatus = "timeout";
      this.audioOn && timeoverAudioContext.play();
      this.$apply();
    }

    answerQuestionTimeout() {
        if (this.question.hasAnswered){
          return;
        }
        this.question.hasAnswered = true;
        api.answerTimeout(this.examId, this.qIndex - 1);
        this.showTimeoutModal();
    }

    /**
     * 绘制倒计时
     */
    redraw(milliSec) {
      // 页面渲染完成
      countDownCanvasContext = wx.createCanvasContext('countDown');
      countDownCanvasContext.setLineWidth(5);
      countDownCanvasContext.setStrokeStyle('#d2d2d2');
      countDownCanvasContext.setLineCap('round')
      countDownCanvasContext.beginPath();//开始一个新的路径
      countDownCanvasContext.arc(40, 40, 30, 0, 2 * Math.PI, false);//设置一个原点(106,106)，半径为100的圆的路径到当前路径
      countDownCanvasContext.stroke();//对当前路径进行描边

      countDownCanvasContext.setLineWidth(5);
      countDownCanvasContext.setStrokeStyle('#6495ED');
      countDownCanvasContext.setLineCap('round')
      countDownCanvasContext.beginPath();//开始一个新的路径
      const eAngle = Math.PI * (2 - (countDownSeconds - parseFloat(milliSec/1000)) * 2 / maxCountSeconds - 1 / 2);
      countDownCanvasContext.arc(40, 40, 30, -Math.PI * 1 / 2, eAngle , false);
      countDownCanvasContext.stroke();//对当前路径进行描边

      countDownCanvasContext.setFontSize(30);
      countDownCanvasContext.setFillStyle('#6495ED');
      countDownCanvasContext.setTextAlign && countDownCanvasContext.setTextAlign('center');
      countDownCanvasContext.setTextBaseline && countDownCanvasContext.setTextBaseline('middle');
      countDownCanvasContext.setStrokeStyle('#6495ED');
      countDownCanvasContext.fillText(countDownSeconds, 40, 40);
      countDownCanvasContext.draw();
    }

    clearTimer() {
      if (answerCountDownTimer !== null) {
        clearInterval(answerCountDownTimer);
        answerCountDownTimer = null;
      }
    }

    initCountDown() {
      let milliSec = 0;
      countDownSeconds = maxCountSeconds;
      this.showCountDown = true;
      this.redraw(milliSec);
      this.clearTimer();
      const that = this;
      this.audioOn && countdownAudioContext.play();
      // 预留 0.5s加载 图片
      delay(0.5).then(() => {
        answerCountDownTimer = setInterval(function () {
          milliSec = milliSec + 10;
          countDownSeconds = countDownSeconds - parseInt(milliSec / 1000);
          if(milliSec >= 1000) {
            milliSec = 0;
          }
          if (countDownSeconds <= 0) {
            that.clearTimer();
            that.answerQuestionTimeout();
          }
          that.redraw(milliSec);
        }, 10);
      });
    }
  }
</script>
